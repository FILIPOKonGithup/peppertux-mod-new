(supertux-level
  (version 3)
  (name (_ "Credits Level"))
  (author "SuperTux Team")
  (contact "supertux.org/contact.html")
  (license "CC-BY-SA 4.0 International")
  (suppress-pause-menu #t)
  (allow-item-pocket "inherit")
  (icon "")
  (icon-locked "")
  (statistics
    (enable-coins #t)
    (enable-secrets #t)
  )
  (sector
    (name "main")
    (init-script "Tux.deactivate();")
    (ambient-light
      (color 1 1 1)
    )
    (background
      (color 1 1 1)
      (speed 0)
      (fill #t)
      (image "/images/background/misc/oiltux.png")
      (z-pos 300)
    )
    (camera
      (name "Camera")
      (mode "manual")
    )
    (gradient
      (name "gradient")
      (top_color 0.88 0.3 0.7)
      (bottom_color 1 1 0.1)
    )
    (music
      (file "/music/misc/credits.music")
    )
    (spawnpoint
      (name "main")
      (x 48)
      (y 672)
    )
    (textscroller
      (file "credits.stxt")
      (x-offset 0)
    )
    (tilemap
      (solid #t)
      (z-pos 0)
      (width 45)
      (height 25)
      (tiles -946 0 7 9 -43 0 13 15 -43 0 16 18 -87 0)
    )
  )
)
